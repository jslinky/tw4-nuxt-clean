{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build:scss": "mkdir -p assets/css && sass assets/scss:assets/css --no-source-map --load-path=node_modules",
    "prepare:css": "npm run build:scss",
    "build": "npm run prepare:css && nuxt build",
    "dev": "npm run prepare:css && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.6.1",
    "@tailwindcss/postcss": "^4.0.0-alpha.11",
    "@tailwindcss/vite": "^4.0.0-alpha.11",
    "clsx": "^2.1.1",
    "nuxt": "^3.10.3",
    "open-props": "^1.6.21",
    "open-props-scss": "^0.7.1",
    "primevue": "^3.52.0",
    "sass": "^1.72.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^4.0.0-alpha.11",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "nuxt-icon": "^0.6.10",
    "nuxt-primevue": "^0.3.1"
  }
}
